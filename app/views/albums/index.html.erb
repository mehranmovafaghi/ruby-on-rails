<h1>My Music Collection</h1>

<h2>
<%= link_to "add new Track", new_album_path %>
</h2><hr />
<% @albums.each do |alb| %>
<p>
<%= link_to 'Edit', {:action => "edit", :id => alb.id} %>

<%= link_to 'Destroy', alb, :confirm => 'Are you sure?', :method => :delete %>

</p><h2>
<%= link_to alb.title, alb %> | <%= alb.created_at %>
</h2>

<hr />
<% end %>

